#Jet Fighter
import turtle
from turtle import *

# Functions

# Constants
RIGHT_EDGE= 400
LEFT_EDGE = -400
BOTTOM_EDGE = -400
TOP_EDGE = 400

# Global Variables

#screen
screen = Screen()
screen.title("Jet Fighter")
screen.bgcolor("skyblue")

# Turtles
jet1 = Turtle()
turtle.addshape(name="Jet.gif", shape=None) 
jet1.shape("Jet.gif") 
jet1.color("black")
jet1.setheading(100)

jet2 = Turtle()
turtle.addshape(name="Jet.gif", shape=None)
jet2.shape("Jet.gif")
jet2.color("green")
jet2.setheading(-100)


# Game Loop
while(True):
    jet1.forward(10) 
